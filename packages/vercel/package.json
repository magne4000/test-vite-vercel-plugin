    "eslint": "^9.2.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vike": "^0.4.171",
    "vite": "^5.2.11"
  },
  "dependencies": {
    "@brillout/libassert": "^0.5.8",
