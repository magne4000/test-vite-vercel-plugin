    "@vercel/nft": "^0.26.4",
    "@vercel/routing-utils": "^3.1.0",
    "esbuild": "^0.20.2",
    "eval": "^0.1.8",
    "fast-glob": "^3.3.2",
    "magicast": "^0.3.4",
    "zod": "^3.23.6"
