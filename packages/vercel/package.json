{
  "name": "vite-plugin-vercel",
  "version": "9.0.3",
  "type": "module",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./universal-middleware": {
      "import": "./dist/universal-middleware.js",
      "require": "./dist/universal-middleware.cjs"
    },
    "./utils": {
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs"
    },
    "./types": {
      "types": "./index.d.ts"
    }
  },
  "types": "./index.d.ts",
  "description": "Vercel adapter for vite",
  "author": "JoÃ«l Charles <joel.charles91@gmail.com>",
  "repository": "https://github.com/magne4000/vite-plugin-vercel",
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepack": "rm -rf dist && pnpm build",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "peerDependencies": {
    "vite": ">=6"
  },
  "devDependencies": {
    "@types/node": "^18.19.67",
    "@vite-plugin-vercel/vike": "workspace:*",
    "rollup": "^4.28.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vite": "^6.0.2"
  },
  "dependencies": {
    "@brillout/libassert": "^0.5.8",
    "@manypkg/find-root": "^2.2.3",
    "@universal-middleware/vercel": "^0.3.0",
    "@vercel/build-utils": "^8.6.0",
    "@vercel/nft": "^0.27.7",
    "@vercel/routing-utils": "^3.1.0",
    "esbuild": "^0.24.0",
    "fast-glob": "^3.3.2",
    "magicast": "^0.3.5",
    "zod": "^3.23.8"
  }
}
